<a href="/movies" class="singleMovieReturn">Återgå till vårat utbud</a>
<div class="singleMovie" id="<%= movie.id %>">
  <img class="singleMovie__img" src="<%= movie.attributes.image.url %>" />
  <h2 class="singleMovie__title"><%= movie.attributes.title %></h2>
  <%- markedIntroText %>
  <div class="singleMovie__rating">
    <div class="singleMovie__rating_star-empty" title="">
      <div class="singleMovie__rating_star-filled"></div>
    </div>
  </div>
</div>
